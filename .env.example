# Database Configuration
# Hostname hoặc IP của máy chủ cơ sở dữ liệu
DB_HOST=
# Cổng của cơ sở dữ liệu (mặc định cho PostgreSQL là 5432)
DB_PORT=
# Tên cơ sở dữ liệu
DB_NAME=
# Tên người dùng cơ sở dữ liệu
DB_USERNAME=
# Mật khẩu người dùng cơ sở dữ liệu
DB_PASSWORD=
# Chế độ SSL (ví dụ: 'disable', 'require')
DB_SSL_MODE=

# JWT Configuration
# Khóa bí mật được sử dụng để ký các token JWT (cần đảm bảo tính bảo mật và độ dài tối thiểu)
JWT_SECRET=
# Thời gian hết hạn của refresh token (ví dụ: tính bằng mili giây)
JWT_REFRESH_EXPIRATION=

# Google OAuth Configuration
# ID client của ứng dụng Google
GOOGLE_CLIENT_ID=
# Khóa bí mật client của ứng dụng Google
GOOGLE_CLIENT_SECRET=
# URL callback sau khi xác thực Google thành công
GOOGLE_CALLBACK_URL=

# External API Keys
# Khóa API cho Finnhub (dịch vụ dữ liệu tài chính)
FINNHUB_API_KEY=

# Application Configuration
# Môi trường ứng dụng (ví dụ: 'development', 'production', 'staging')
APP_ENV=
# Bật/tắt chế độ debug (true/false)
APP_DEBUG=
# URL phía client của ứng dụng
APP_URL=
# URL phía API/server của ứng dụng
API_URL=
# URL Web Socket của ứng dụng
WS_URL=

# Monitoring
# Bật/tắt Prometheus monitoring (true/false)
PROMETHEUS_ENABLED=
# Mật khẩu quản trị viên cho Grafana
GRAFANA_ADMIN_PASSWORD=

# Security
# Danh sách các origin được phép cho CORS, phân cách bằng dấu phẩy
CORS_ALLOWED_ORIGINS=
# Số lượng request tối đa mỗi phút cho giới hạn tốc độ
RATE_LIMIT_REQUESTS_PER_MINUTE=